  },
  "types": "dist/ViewerPlugin.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bestkolobok/vue3-jsoneditor"
  },
  "bugs": {
    "url": "https://github.com/bestkolobok/vue3-jsoneditor/issues"
  },
  "scripts": {
