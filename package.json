    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsoneditor": "^9.5.1",
    "@types/node": "^16.11.41",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.0",
    "typescript": "~4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-env-config": "^2.0.0",
    "vue-tsc": "^0.38.1"
  },
  "keywords": [
    "vue3",
    "vue",
    "jsoneditor",
    "jsonformatter",
    "jsonviewer",
    "json",
    "editor",
    "viewer",
    "formatter"
