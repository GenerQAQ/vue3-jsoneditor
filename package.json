  "scripts": {
    "dev": "vite",
    "deploy": "sh deploy/patch.sh",
    "deploy:minor": "sh deploy/minor.sh",
    "deploy:major": "sh deploy/major.sh",
    "build": "run-p type-check build-only",
    "build:gh": "VITECONFIG_build_lib=false VITECONFIG_build_cssCodeSplit=true VITECONFIG_build_outDir=gh-dist VITECONFIG_base=vue3-jsoneditor run-p build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "jsoneditor": "^9.9.0",
