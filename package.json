    "sass": "^1.53.0",
    "sass-loader": "^13.0.0",
    "typescript": "~4.7.4",
    "vite": "^2.9.12",
    "vue-tsc": "^0.38.1"
  },
  "keywords": [
    "vue3",
    "vue",
    "jsoneditor",
    "jsonformatter",
    "jsonviewer",
